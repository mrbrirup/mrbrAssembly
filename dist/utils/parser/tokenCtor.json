{
    "markerRegexes" : [
        { "group": "Whitespace", "name": "whitespace", "rx": "( |\\t|\\f|\\r|\\n)\\1*", "flags":"g" },
        { "group": "Quote", "name": "quotes", "rx": "(\\\"|'|`)" , "flags":"g"},
        { "group": "Comment", "name": "comments", "rx": "([/]{2}|[/]\\*|\\*\\/)", "flags":"g" },
        { "group": "Whitespace", "name": "crlf", "rx": "(\\r\\n)\\1*" , "flags":"g"},
        { "group": "Block", "name": "blocks", "rx": "([{}[\\]()]{1})" , "flags":"g"},
        { "group": "Operator", "name": "operators", "rx": "(;|,|=|\\/|\\.)" , "flags":"g" }
    ],
    "initialBlocks": {
        "/*": "blockCommentStart",
        "\"": "doubleQuoteText",
        "'": "singleQuoteText",
        "//": "singleLineComment",
        "`": "tickQuoteText"
    },
    "identifier":"^([a-z][a-z0-9_$]*)$"    ,
    "whitespace": {
        "\r\n":"carriageReturnNewLine",
        " ":"space",
        "\t":"tab",
        "\n":"newline",
        "\r":"carriageReturn",
        "\f":"formFeed"
    },
    "blocks": {
        "{":"openBrace",
        "}":"closeBrace",
        "[":"openBracket",
        "]":"closeBracket",
        "(":"openParenthesis",
        ")":"closeParenthesis"
    },
    "keywords": [
        "class",
        "constructor"
    ],
    "objectMethods": [
        "apply",
        "call",
        "forEach"
    ],
    "operators": {
        ";": "semicolon",
        ",": "comma",
        "=": "assign",
        "/": "divide",
        ".": "dot"
    }
}