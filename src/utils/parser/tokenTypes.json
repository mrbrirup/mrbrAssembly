{
        "markerRegexes" : [
            { "group": "Whitespace", "name": "whitespace", "rx": "( |\\t|\\f|\\r|\\n)\\1*", "flags":"g"  },
            { "group": "Quote", "name": "quotes", "rx": "(\"|'|`)+", "flags":"g"  },
            { "group": "Comment", "name": "comments", "rx": "([/]{2}|[/]\\*|\\*\\/)", "flags":"g"  },
            { "group": "Number", "name": "exponent", "rx": "(\\b[0-9]+[.][0-9]e[0-9]+\\b|\\b[0-9]e[0-9]+\\b)", "flags":"ig"  },
            { "group": "Number", "name": "hex", "rx": "(\\b0x[0-9a-f]+\\b)", "flags":"ig"  },
            { "group": "Number", "name": "octal", "rx": "(\\b0o{0,1}[0-7]+\\b)", "flags":"ig"  },
            { "group": "Number", "name": "binary", "rx": "(\\b0b{1}[0-1]+\\b)", "flags":"ig"  },
            { "group": "Number", "name": "float", "rx": "(\\b[0-9]+\\.[0-9]+\\b)" , "flags":"g" },
            { "group": "Number", "name": "integer", "rx": "(\\b[0-9]+\\b)", "flags":"g"  },
            { "group": "Number", "name": "infinity", "rx": "(\\bInfinity\\b)" , "flags":"g" },
            { "group": "Number", "name": "NaN", "rx": "(\\bNaN\\b)" , "flags":"g" },
            { "group": "Whitespace", "name": "crlf", "rx": "(\\r\\n)\\1*" , "flags":"g" },
            { "group": "Block", "name": "blocks", "rx": "([{}[\\]()]{1})" , "flags":"g" },
            { "group": "Operator", "name": "operators", "rx": "(;|,|=|\\/|\\.)" , "flags":"g" },
            { "group": "Operator", "name": "operators", "rx": "(>>>=|>>=|<<=|===|!==|[.]{3}|>{3}|<<|!=|<=|>>|>=|[+]{2}|--|==|=>|[|]=|\\^=|&=|\\+=|-=|\\*=|\\/=|%=|\\|\\||&&|;|,|\\?|:|\\^|&|=|<|>|\\+|-|\\*|\\/|%|!|~|\\.)" , "flags":"g" }            
        ],
    "initialBlocks": {
        "/*": "blockCommentStart",
        "\"": "doubleQuoteText",
        "'": "singleQuoteText",
        "//": "singleLineComment",
        "`": "tickQuoteText",
        "=": "equals",
        ">": "greaterThan",
        "<": "lessThan",
        "|": "pipe",
        "^": "carat",
        "&": "ampersand",
        "+": "add",
        "-": "substract",
        "*": "multiply",
        "/": "divide",
        "%": "modulos",
        ";": "semiColon",
        ",": "comma",
        "?": "question",
        ":": "colon",
        "!": "exclamation",
        "~": "tilde",
        ".": "dot"    
    },
    "identifier":"^([a-z][a-z0-9_$]*)$"    ,
    "whitespace": {
        "\r\n":"carriageReturnNewLine",
        " ":"space",
        "\t":"tab",
        "\n":"newline",
        "\r":"carriageReturn",
        "\f":"formFeed"
    },
    "blocks": {
        "{":"openBrace",
        "}":"closeBrace",
        "[":"openBracket",
        "]":"closeBracket",
        "(":"openParenthesis",
        ")":"closeParenthesis"
    },
